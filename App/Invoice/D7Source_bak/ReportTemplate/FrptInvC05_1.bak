<TfrxReport Name="" Version="3.15" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Printer="預設" ReportOptions.CreateDate="38313.5793211806" ReportOptions.Description.Text="" ReportOptions.LastChange="38908.5395475231" ScriptLanguage="PascalScript" ScriptText.Text="&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  aText: String;&#13;&#10;begin&#13;&#10;  Set( '發票號碼', '''' + Copy( &#60;frxC05_1.&#34;發票號碼&#34;&#62;, 9, 2 ) + ''''  );&#13;&#10;  {}&#13;&#10;    CheckBox1.Checked := False;&#13;&#10;    CheckBox2.Checked := False;&#13;&#10;    CheckBox3.Checked := False;&#13;&#10;  {}&#13;&#10;  if ( &#60;frxC05_1.&#34;做廢否&#34;&#62; = 'N' ) then&#13;&#10;  begin&#13;&#10;    Set( '發票日期', '''' + FormatDateTime( 'mmdd', &#60;frxC05_1.&#34;發票日期&#34;&#62; ) + '''' );&#13;&#10;    Set( '發票統一編號', '''' + &#60;frxC05_1.&#34;發票統編&#34;&#62; + '''' );&#13;&#10;    if ( &#60;frxC05_1.&#34;發票統編&#34;&#62; = '' ) and ( &#60;ComBine&#62; = '1' ) then&#13;&#10;    begin&#13;&#10;      { 非營業人稅額採 合併計算, 則 發票銷售額 = 發票總金額, 在報表頁面的運算式設定 }&#13;&#10;      Set( '發票銷售額', null );&#13;&#10;      Set( '發票稅額', null );&#13;&#10;      Set( '發票發票額', &#60;frxC05_1.&#34;發票金額&#34;&#62; );&#13;&#10;    end else&#13;&#10;    begin&#13;&#10;      Set( '發票銷售額', &#60;frxC05_1.&#34;銷售額&#34;&#62; );&#13;&#10;      Set( '發票稅額',  &#60;frxC05_1.&#34;稅額&#34;&#62; );&#13;&#10;      Set( '發票發票額', &#60;frxC05_1.&#34;發票金額&#34;&#62; );&#13;&#10;    end;&#13;&#10;    {}&#13;&#10;    Set( '發票稅別', '''' + '' + '''' );&#13;&#10;    {}&#13;&#10;    if ( &#60;frxC05_1.&#34;稅別&#34;&#62; = '1' ) then&#13;&#10;      CheckBox1.Checked := True&#13;&#10;    else if ( &#60;frxC05_1.&#34;稅別&#34;&#62; = '2' ) then&#13;&#10;      CheckBox2.Checked := True&#13;&#10;    else if ( &#60;frxC05_1.&#34;稅別&#34;&#62; = '3' ) then&#13;&#10;      CheckBox3.Checked := True;&#13;&#10;    {}&#13;&#10;  end else&#13;&#10;  if ( &#60;frxC05_1.&#34;做廢否&#34;&#62; = 'Y' ) then&#13;&#10;  begin&#13;&#10;    Set( '發票日期', '''' + '----' + '''' );&#13;&#10;    Set( '發票統一編號', '''' + '--------' + '''' );&#13;&#10;    Set( '發票銷售額', '''' + '---------' + '''' );&#13;&#10;    Set( '發票稅額', '''' + '---------' + '''' );&#13;&#10;    Set( '發票發票額', '''' + '--------' + '''' );&#13;&#10;    Set( '發票稅別', '''' + '本  號  作  廢' + '''' );&#13;&#10;  end else&#13;&#10;  if ( &#60;frxC05_1.&#34;做廢否&#34;&#62; = 'S' ) then&#13;&#10;  begin&#13;&#10;    Set( '發票日期', '''' + '----' + '''' );&#13;&#10;    Set( '發票統一編號', '''' + '--------' + '''' );&#13;&#10;    Set( '發票銷售額', '''' + '--------' + '''' );&#13;&#10;    Set( '發票稅額', '''' + '--------' + '''' );&#13;&#10;    Set( '發票發票額', '''' + '--------' + '''' );&#13;&#10;    Set( '發票稅別', '''' + '本  號  空  白' + '''' );&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PageHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if ( VarToStr( &#60;發票年&#62; ) = '' ) then&#13;&#10;    Set( '發票年', '''' + FormatDateTime( 'eee', &#60;frxC05_1.&#34;發票日期&#34;&#62; ) + '''' );&#13;&#10;  if ( VarToStr( &#60;發票月&#62; ) = '' ) then&#13;&#10;    Set( '發票月', '''' + FormatDateTime( 'mm', &#60;frxC05_1.&#34;發票日期&#34;&#62; ) + '''' );&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupFooter1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Memo3.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo4.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo5.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo6.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo115.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo116.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo117.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo118.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo119.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo120.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo121.Visible := MasterData1.DataSet.Eof;&#13;&#10;  Memo122.Visible := MasterData1.DataSet.Eof;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  Set( 'ComBineText', '''' + '分開' + '''' );&#13;&#10;  if ( &#60;ComBine&#62; = '1' ) then Set( 'ComBineText', '''' + '合併' + '''' );&#13;&#10;end." PropData="">
