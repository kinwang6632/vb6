<TfrxReport Name="" Version="3.15" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Printer="財務部-1022n" ReportOptions.CreateDate="38579.6493757407" ReportOptions.Description.Text="" ReportOptions.LastChange="39952.6126229745" ScriptLanguage="PascalScript" ScriptText.Text="&#13;&#10;var&#13;&#10;  aChineseNumber: array [0..9] of String;&#13;&#10;&#13;&#10;{ ------------------------------------------------------------------------------------ }&#13;&#10;&#13;&#10;function GetDescription(aIndex: Integer): String;&#13;&#10;var&#13;&#10;  aDescription: String;&#13;&#10;  aYear, aMonth, aDay: Integer;&#13;&#10;  aYear2, aMonth2, aDay2: Integer;&#13;&#10;begin&#13;&#10;  aYear := 0;&#13;&#10;  aMonth := 0;&#13;&#10;  aDay := 0;&#13;&#10;  {}&#13;&#10;  aYear2 := 0;&#13;&#10;  aMonth2 := 0;&#13;&#10;  aDay2 := 0;&#13;&#10;  {}&#13;&#10;  if ( aIndex = 1 ) then&#13;&#10;  begin&#13;&#10;    aDescription := &#60;frxA05_1.&#34;品名(一)&#34;&#62;;&#13;&#10;    if ( &#60;frxA05_1.&#34;有效日起(一)&#34;&#62; &#60;&#62; 0 ) then&#13;&#10;      DecodeDate( &#60;frxA05_1.&#34;有效日起(一)&#34;&#62;, aYear, aMonth, aDay );&#13;&#10;    if ( &#60;frxA05_1.&#34;有效日迄(一)&#34;&#62; &#60;&#62; 0 ) then&#13;&#10;      DecodeDate( &#60;frxA05_1.&#34;有效日迄(一)&#34;&#62;, aYear2, aMonth2, aDay2 );&#13;&#10;  end else&#13;&#10;  if ( aIndex = 2 ) then&#13;&#10;  begin&#13;&#10;    aDescription := &#60;frxA05_1.&#34;品名(二)&#34;&#62;;&#13;&#10;    if ( &#60;frxA05_1.&#34;有效日起(二)&#34;&#62; &#60;&#62; 0 ) then&#13;&#10;      DecodeDate( &#60;frxA05_1.&#34;有效日起(二)&#34;&#62;, aYear, aMonth, aDay );&#13;&#10;    if ( &#60;frxA05_1.&#34;有效日迄(二)&#34;&#62; &#60;&#62; 0 ) then&#13;&#10;      DecodeDate( &#60;frxA05_1.&#34;有效日迄(二)&#34;&#62;, aYear2, aMonth2, aDay2 );&#13;&#10;  end else&#13;&#10;  if ( aIndex = 3 ) then&#13;&#10;  begin&#13;&#10;    aDescription := &#60;frxA05_1.&#34;品名(三)&#34;&#62;;&#13;&#10;    if ( &#60;frxA05_1.&#34;有效日起(三)&#34;&#62; &#60;&#62; 0 ) then&#13;&#10;      DecodeDate( &#60;frxA05_1.&#34;有效日起(三)&#34;&#62;, aYear, aMonth, aDay );&#13;&#10;    if ( &#60;frxA05_1.&#34;有效日迄(三)&#34;&#62; &#60;&#62; 0 ) then&#13;&#10;      DecodeDate( &#60;frxA05_1.&#34;有效日迄(三)&#34;&#62;, aYear2, aMonth2, aDay2 );&#13;&#10;  end else&#13;&#10;  if ( aIndex = 4 ) then&#13;&#10;  begin&#13;&#10;    aDescription := &#60;frxA05_1.&#34;品名(四)&#34;&#62;;&#13;&#10;    if ( &#60;frxA05_1.&#34;有效日起(四)&#34;&#62; &#60;&#62; 0 ) then&#13;&#10;      DecodeDate( &#60;frxA05_1.&#34;有效日起(四)&#34;&#62;, aYear, aMonth, aDay );&#13;&#10;    if ( &#60;frxA05_1.&#34;有效日迄(四)&#34;&#62; &#60;&#62; 0 ) then&#13;&#10;      DecodeDate( &#60;frxA05_1.&#34;有效日迄(四)&#34;&#62;, aYear2, aMonth2, aDay2 );&#13;&#10;  end else&#13;&#10;  if ( aIndex = 5 ) then&#13;&#10;  begin&#13;&#10;    aDescription := &#60;frxA05_1.&#34;品名(五)&#34;&#62;;&#13;&#10;    if ( &#60;frxA05_1.&#34;有效日起(五)&#34;&#62; &#60;&#62; 0 ) then&#13;&#10;      DecodeDate( &#60;frxA05_1.&#34;有效日起(五)&#34;&#62;, aYear, aMonth, aDay );&#13;&#10;    if ( &#60;frxA05_1.&#34;有效日起(五)&#34;&#62; &#60;&#62; 0 ) then&#13;&#10;      DecodeDate( &#60;frxA05_1.&#34;有效日迄(五)&#34;&#62;, aYear2, aMonth2, aDay2 );&#13;&#10;  end;&#13;&#10;  Result := aDescription;&#13;&#10;  if ( aYear &#62; 0 ) then&#13;&#10;    Result := ( Result + Format( '(%d/%d/%d', [aYear-1911, aMonth+0, aDay+0] ) );&#13;&#10;  if ( aYear2 &#62; 0 ) then&#13;&#10;    Result := ( Result + '-' + Format( '%d/%d/%d)', [aYear2-1911, aMonth2+0, aDay2+0] ) );&#13;&#10;end;&#13;&#10;&#13;&#10;{ ------------------------------------------------------------------------------------ }&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  aYear, aMonth, aDay: Integer;&#13;&#10;  aIndex, aNum: Integer;&#13;&#10;  aObject: TfrxMemoView;&#13;&#10;  aStrNum: String;&#13;&#10;begin&#13;&#10;&#13;&#10;  { 是否列印發票抬頭 }&#13;&#10;  Memo5.Visible := ( Get( '列印發票抬頭' ) = 'Y' ) or ( &#60;frxA05_1.&#34;統一編號&#34;&#62; &#60;&#62; '' );&#13;&#10;  Memo49.Visible := Memo5.Visible;&#13;&#10;&#13;&#10;  { 組合收費品名  }&#13;&#10;  Set( '品名(一)', '''' + GetDescription(1) + '''' );&#13;&#10;  Set( '品名(二)', '''' + GetDescription(2) + '''' );&#13;&#10;  Set( '品名(三)', '''' + GetDescription(3) + '''' );&#13;&#10;  Set( '品名(四)', '''' + GetDescription(4) + '''' );&#13;&#10;  Set( '品名(五)', '''' + GetDescription(5) + '''' );&#13;&#10;&#13;&#10;  { 發票日期 }&#13;&#10;  DecodeDate( &#60;frxA05_1.&#34;發票日期&#34;&#62;, aYear, aMonth, aDay );&#13;&#10;  Set( '發票日(年)', Format( '%d', [aYear-1911] ) );&#13;&#10;  Set( '發票日(月)', Format( '%d', [aMonth+0] ) ); { 不加 0 , 印到表上會不正確, ??? }&#13;&#10;  Set( '發票日(日)', Format( '%d', [aDay+0] ) ); { 不加 0 , 印到表上會不正確, ??? }&#13;&#10;&#13;&#10;  { 客編 }&#13;&#10;  Memo32.Visible := ( &#60;frxA05_1.&#34;客戶代碼&#34;&#62; &#60;&#62; '' );&#13;&#10;&#13;&#10;  { 稅別 }&#13;&#10;  Set( '應稅', '''''' );&#13;&#10;  Set( '零稅率', '''''' );&#13;&#10;  Set( '免稅', '''''' );&#13;&#10;&#13;&#10;  if ( &#60;frxA05_1.&#34;稅別&#34;&#62; = '1' ) then&#13;&#10;    Set( '應稅', '''V'''  )&#13;&#10;  else if ( &#60;frxA05_1.&#34;稅別&#34;&#62; ) = '2' then&#13;&#10;    Set( '零稅率', '''V''' )&#13;&#10;  else if ( &#60;frxA05_1.&#34;稅別&#34;&#62; ) = '3' then&#13;&#10;    Set( '免稅', '''V''' );&#13;&#10;&#13;&#10;&#13;&#10;  { 發票金額轉中文數字 }&#13;&#10;&#13;&#10;  aStrNum := Format( '%.8d', [&#60;frxA05_1.&#34;發票總金額&#34;&#62;] );&#13;&#10;  for aIndex := 1 to Length( aStrNum ) do&#13;&#10;  begin&#13;&#10;    aNum := StrToInt( Copy( aStrNum, aIndex, 1 ) );&#13;&#10;    aObject := nil;&#13;&#10;    case aIndex of&#13;&#10;      1: Set( '仟萬', '''' + aChineseNumber[aNum] + '''' );&#13;&#10;      2: Set( '佰萬', '''' + aChineseNumber[aNum] + '''' );&#13;&#10;      3: Set( '拾萬', '''' + aChineseNumber[aNum] + '''' );&#13;&#10;      4: Set( '萬', '''' + aChineseNumber[aNum] + '''' );&#13;&#10;      5: Set( '仟', '''' + aChineseNumber[aNum] + '''' );&#13;&#10;      6: Set( '佰', '''' + aChineseNumber[aNum] + '''' );&#13;&#10;      7: Set( '拾', '''' + aChineseNumber[aNum] + '''' );&#13;&#10;      8: Set( '個', '''' + aChineseNumber[aNum] + '''' );&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;{ ------------------------------------------------------------------------------------ }&#13;&#10;&#13;&#10;begin&#13;&#10;  aChineseNumber[0] := '零';&#13;&#10;  aChineseNumber[1] := '壹';&#13;&#10;  aChineseNumber[2] := '貳';&#13;&#10;  aChineseNumber[3] := '參';&#13;&#10;  aChineseNumber[4] := '肆';&#13;&#10;  aChineseNumber[5] := '伍';&#13;&#10;  aChineseNumber[6] := '陸';&#13;&#10;  aChineseNumber[7] := '柒';&#13;&#10;  aChineseNumber[8] := '捌';&#13;&#10;  aChineseNumber[9] := '玖';&#13;&#10;end." PropData="">
